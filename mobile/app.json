{
  "expo": {
    "name": "JudgeX",
    "slug": "judgex-mobile",
    "version": "1.0.0",
    "scheme": "judgex",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "backgroundColor": "#0D1117"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.judgex.mobile"
    },
    "android": {
      "package": "com.judgex.mobile"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-asset",
      "expo-font"
    ]
  }
}
